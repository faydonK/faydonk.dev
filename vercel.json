{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/twitch",
      "destination": "https://www.twitch.tv/faydonKwin",
      "permanent": true
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/faydonKwin",
      "permanent": true
    },
    {
      "source": "/instagram",
      "destination": "https://instagram.com/milo_tout_cours/",
      "permanent": true
    },
    {
      "source": "/aircodes",
      "destination": "https://aircodes.net",
      "permanent": true
    },
    {
      "source": "/konect",
      "destination": "https://konect.gg/faydonK",
      "permanent": true
    },
    {
      "source": "/github",
      "destination": "https://github.com/faydonK",
      "permanent": true
    }
  ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
