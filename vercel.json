{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/twitch",
      "destination": "https://www.twitch.tv/faydonKwin",
      "permanent": true
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/faydonKwin",
      "permanent": true
    },
    {
      "source": "/instagram",
      "destination": "https://instagram.com/milo_tout_cours/",
      "permanent": true
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/HPcQWqPgD8",
      "permanent": true
    },
    {
      "source": "/konect",
      "destination": "https://konect.gg/faydonK",
      "permanent": true
    },
    {
      "source": "/github",
      "destination": "https://github.com/faydonK",
      "permanent": true
    }
  ],

"routes": [ 
  { "handle": "filesystem" },
  { "src": "/(.*)", "status": 404, "dest": "public/404.html" }
],
  
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}